        "@angular/router": "^4.4.6",
        "@angular/tsc-wrapped": "^4.4.6",
        "@angular/upgrade": "^4.4.6",
        "@asymmetrik/ngx-leaflet": "^2.5.3",
        "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.7",
        "ajv": "^5.5.1",
