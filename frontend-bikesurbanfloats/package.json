{
  "name": "frontend-bikesurbanfloats",
  "version": "0.0.1",
  "scripts": {
    "start": "electron ./build",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "webpack --config src/webpack/main.dev.js --bail",
    "build:renderer": "webpack --config src/webpack/renderer.dev.js --bail",
    "dist": "webpack --config src/webpack/main.prod.js --bail && webpack --config src/webpack/renderer.prod.js --bail && build"
  },
  "build": {
    "win": {
      "target": "7z",
      "files": {
        "from": "build"
      }
    }
  },
  "devDependencies": {
    "@angular/animations": "^4.4.0-RC.0",
    "@angular/common": "^4.4.0-RC.0",
