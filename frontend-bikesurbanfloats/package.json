    "uglify-es": "^3.1.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "webpack": "^3.5.5",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.17"
  }
